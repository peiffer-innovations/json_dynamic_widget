{
  "type": "set_widget",
  "args": {
    "widgets": {
      "variableWidgetList": [
        {
          "type": "text",
          "args": {
            "text": "This is a text."
          }
        }
      ]
    },
    "child": {
      "type": "scaffold",
      "args": {
        "appBar": {
          "type": "app_bar",
          "args": {
            "title": {
              "type": "text",
              "args": {
                "text": "VariableWidgetList"
              }
            }
          }
        },
        "body": {
          "type": "list_view",
          "listen": [
            "variableWidgetList"
          ],
          "args": {
            "children": "${variableWidgetList}"
          }
        }
      }
    }
  }
}